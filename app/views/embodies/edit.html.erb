<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.embody')) -%></h1>
<div id="content_list">

<%= form_for(@embody) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.models.expression') -%><br />
    <%= f.text_field :expression_id -%>
  </p>

  <p>
    <%= f.label t('activerecord.models.manifestation') -%><br />
    <%= f.text_field :manifestation_id -%>
  </p>

  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>
</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.show'), @embody -%></li>
    <li><%= link_to t('page.back'), embodies_path -%></li>
  </ul>
</div>
