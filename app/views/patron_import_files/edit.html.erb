<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.patron_import_file')) -%></h1>
<div id="content_list">

<%= form_for(@patron_import_file) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :patron_import_file_name -%><br />
    <%= f.text_field :patron_import_file_name -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @patron_import_file -%></li>
    <li><%= link_to t('page.back'), patron_import_files_path -%></li>
  </ul>
</div>
