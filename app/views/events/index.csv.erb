library,title,note,started_at,ended_at
<%- @events.each do |event| -%>
"<%=h event.library.name.gsub(/"/, '""') -%>","<%=h event.title.gsub(/"/, '""') -%>","<%=h event.note.to_s.gsub(/"/, '""') -%>","<%=h event.started_at.to_s.gsub(/"/, '""') -%>","<%=h event.ended_at.to_s.gsub(/"/, '""') -%>"<%= "\n" -%>
<%- end -%>
