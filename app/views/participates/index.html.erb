<div id="content_detail">
<h1>Listing participates</h1>
<div id="content_list">

<table id="index">
  <tr>
    <th>Patron</th>
    <th>Event</th>
  </tr>

<% @participates.each do |participate| %>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to h(participate.patron.full_name), participate.patron %>
    <td><%= link_to h(participate.event.title), participate.event %>
    <td><%= link_to 'Show', participate %></td>
    <td><%= link_to 'Edit', edit_participate_path(participate) %></td>
    <td><%= link_to 'Destroy', participate, :confirm => t('page.are_you_sure'), :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= will_paginate(@participates) -%>
</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to 'New participate', new_participate_path %></li>
  </ul>
</div>
