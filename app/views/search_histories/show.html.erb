<div id="sidebar">
  <ul>
    <li><%= link_to t('page.back'), search_histories_path -%></li>
  </ul>
</div>

<div class="inner">
<div class="inner">
<p>
  <b>Operation:</b>
  <%=h @search_history.operation -%>
</p>

<p>
