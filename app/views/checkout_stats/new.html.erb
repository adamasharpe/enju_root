<h1>New checkout_stat</h1>

<% form_for(@checkout_stat) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :from_date %><br />
    <%= f.datetime_select :from_date %>
  </p>
  <p>
    <%= f.label :to_date %><br />
    <%= f.datetime_select :to_date %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note, :class => 'resource_textarea' %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', checkout_stats_path %>
