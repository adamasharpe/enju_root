<h1><%= t('page.editing', :model => t('activerecord.models.reserve_stat_has_user')) -%></h1>

<% form_for(@reserve_stat_has_user) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :user_reserve_stat_id -%><br />
    <%= f.text_field :user_reserve_stat_id -%>
  </p>
  <p>
    <%= f.label :user_id -%><br />
    <%= f.text_field :user_id -%>
  </p>
  <p>
    <%= f.label :reserves_count -%><br />
    <%= f.text_field :reserves_count -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>

<%= link_to t('page.show'), @reserve_stat_has_user -%> |
<%= link_to t('page.back'), reserve_stat_has_users_path -%>
