<h1><%= t('page.new', :model => t('activerecord.models.subject_has_classification')) -%></h1>

<%= error_messages_for :subject_has_classification -%>

<%- form_for(@subject_has_classification) do |f| -%>
  <p>
    <b>Subject</b><br />
    <%= f.text_field :subject_id -%>
  </p>

  <p>
    <b>Decimal classification</b><br />
    <%= f.text_field :classification_id -%>
  </p>

  <p>
    <%= f.submit t('page.create') -%>
  </p>
<%- end -%>

<%= link_to t('page.back'), subject_has_classifications_path -%>
