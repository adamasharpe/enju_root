<div id="submenu">
  <ul>
    <li><%= link_to t('page.new', :model => t('activerecord.models.subject_has_classification')), new_subject_has_classification_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= t('page.listing', :model => t('activerecord.models.subject_has_classifications')) -%></h1>

<table>
  <tr>
    <th><%= t('page.subject') -%></th>
    <th><%= ('Classification') -%></th>
  </tr>

<%- for subject_has_classification in @subject_has_classifications -%>
  <tr>
    <td><%=h subject_has_classification.subject_id -%></td>
    <td><%=h subject_has_classification.classification_id -%></td>
    <td><%= link_to t('page.show'), subject_has_classification -%></td>
    <td><%= link_to t('page.edit'), edit_subject_has_classification_path(subject_has_classification) -%></td>
    <td><%= link_to t('page.destroy'), subject_has_classification, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
