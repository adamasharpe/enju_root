<h1>Listing item_has_items</h1>

<table>
  <tr>
    <th>From item</th>
    <th>To item</th>
    <th>Item Relationship type</th>
    <th>Position</th>
  </tr>

<% for item_has_item in @item_has_items %>
  <tr>
    <td><%=h item_has_item.from_item_id %></td>
    <td><%=h item_has_item.to_item_id %></td>
    <td><%=h item_has_item.item_relationship_type_id %></td>
    <td><%=h item_has_item.position %></td>
    <td><%= link_to 'Show', item_has_item %></td>
    <td><%= link_to 'Edit', edit_item_has_item_path(item_has_item) %></td>
    <td><%= link_to 'Destroy', item_has_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New item_has_item', new_item_has_item_path %>
