<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.work')) -%></h1>
<div id="content_list">

<%= form_for(@work) do |f| -%>
  <%= render :partial => 'form', :locals => {:f => f} -%>
  <div class="actions">
    <%= f.submit -%>
  </div>
<%- end -%>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.show'), @work -%></li>
    <li><%= link_to t('page.back'), works_path -%></li>
  </ul>
</div>
