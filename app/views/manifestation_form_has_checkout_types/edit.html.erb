<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @manifestation_form_has_checkout_type -%></li>
    <li><%= link_to t('page.back'), manifestation_form_has_checkout_types_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= t('page.editing', :model => t('activerecord.models.manifestation_form_has_checkout_type')) -%></h1>

<%- form_for(@manifestation_form_has_checkout_type) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :manifestation_form_id -%><br />
    <%= f.select(:manifestation_form_id, @manifestation_forms.collect{|m| [m.display_name, m.id]}) -%>
  </p>
  <p>
    <%= f.label :checkout_type_id -%><br />
    <%= f.select(:checkout_type_id, @checkout_types.collect{|c| [c.display_name, c.id]}) -%>
  </p>
  <p>
    <%= f.label :note -%><br />
    <%= f.text_area :note, :class => 'resource_textarea' -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
