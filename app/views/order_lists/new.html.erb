<div id="submenu">
  <ul>
    <li><%= link_to ('Back'), order_lists_path -%></li>
  </ul>
</div>

<div id="content_detail">
  <h1><%= ('New order_list') -%></h1>

<%- form_for(@order_list) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :title -%><br />
    <%= f.text_field :title, :class => 'resource_title' -%>
  </p>
  <p>
    <b><%= ('Bookstore') -%></b><br />
    <%= f.select(:bookstore_id, @bookstores.collect{|b| [b.name, b.id]}) -%>
  </p>
  <p>
    <%= f.label :note -%><br />
    <%= f.text_area :note, :class => 'resource_textarea' -%>
  </p>
  <p>
    <%= f.submit Create -%>
  </p>
<%- end -%>

</div>
