<div id="main">
<h1 class="title"><%= t('page.listing', :model => t('activerecord.models.checkout_stat_has_manifestation')) -%></h1>
<div class="inner">

<table class="index">
  <tr>
    <th>Checkout stat</th>
    <th>Manifestation</th>
    <th>Checkouts count</th>
  </tr>

<% for checkout_stat_has_manifestation in @checkout_stat_has_manifestations -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%=h checkout_stat_has_manifestation.manifestation_checkout_stat_id -%></td>
    <td><%=h checkout_stat_has_manifestation.manifestation_id -%></td>
    <td><%=h checkout_stat_has_manifestation.checkouts_count -%></td>
    <td><%= link_to t('page.show'), checkout_stat_has_manifestation -%></td>
    <td><%= link_to t('page.edit'), edit_checkout_stat_has_manifestation_path(checkout_stat_has_manifestation) -%></td>
    <td><%= link_to t('page.destroy'), checkout_stat_has_manifestation, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<% end -%>
</table>

<%= will_paginate(@checkout_stat_has_manifestations, :previous_label => "&#171; " + t('page.previous'), :next_label => "&#187; " + t('page.next')) -%>

</div>
</div>

<div id="sidebar">
  <ul>
    <li><%= link_to t('page.new', :model => t('activerecord.models.checkout_stat_has_manifestation')), new_checkout_stat_has_manifestation_path -%></li>
  </ul>
</div>
