<div id="submenu">
  <ul>
    <li><%= link_to t('page.edit'), edit_imported_patron_file_path(@imported_patron_file) -%></li>
    <li><%= link_to t('page.back'), imported_patron_files_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.imported_patron_file')) -%></h1>
<div id="content_list">
<p>
  <strong><%= t('activerecord.attributes.imported_patron_file.imported_patron_file_name') -%>:</strong>
  <%= link_to h(@imported_patron_file.imported_patron_file_name), imported_patron_file_path(@imported_patron_file, :format => :download) -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.imported_patron_file.imported_patron_content_type') -%>:</strong>
  <%= @imported_patron_file.imported_patron_content_type -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.imported_patron_file.imported_patron_file_size') -%>:</strong>
  <%= @imported_patron_file.imported_patron_file_size -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.imported_patron_file.file_hash') -%>:</strong>
  <%= @imported_patron_file.file_hash -%>
</p>

<p>
  <strong><%= t('page.created_at') -%>:</strong>
  <%= l(@imported_patron_file.created_at) if @imported_patron_file.created_at -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.imported_patron_file.imported_at') -%>:</strong>
  <%= l(@imported_patron_file.imported_at) if @imported_patron_file.imported_at -%>
</p>

<p>
  <strong><%= t('activerecord.models.user') -%>:</strong>
  <%= link_to h(@imported_patron_file.user.login), user_path(@imported_patron_file.user.login) -%>
</p>

<table class="index">
  <%- @imported_patron_file.imported_objects.each do |object| -%>
    <tr>
      <td><%= link_to h(object.importable.full_name), object.importable -%></td>
      <td><%= link_to h(object.importable.user.login), user_path(object.importable.login) if object.importable.user -%></td>
      <td><%= l(object.created_at) -%></td>
    </tr>
  <%- end -%>
</table>

</div>
</div>
