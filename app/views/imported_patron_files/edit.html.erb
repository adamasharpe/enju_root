<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @imported_patron_file -%></li>
    <li><%= link_to t('page.back'), imported_patron_files_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.imported_patron_file')) -%></h1>
<div id="content_list">

<%- form_for(@imported_patron_file) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :filename -%><br />
    <%= f.text_field :filename -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>
