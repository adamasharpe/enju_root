<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @imported_resource_file -%></li>
    <li><%= link_to t('page.back'), imported_resource_files_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= t('page.editing', :model => t('activerecord.models.imported_resource_file')) -%></h1>

<%- form_for(@imported_resource_file) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :filename -%><br />
    <%= f.text_field :filename -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
