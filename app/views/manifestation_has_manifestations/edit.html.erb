<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.manifestation_has_manifestation')) -%></h1>
<div id="content_list">

<% form_for(@manifestation_has_manifestation) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_manifestation_id -%><br />
    <%= f.text_field :from_manifestation_id -%>
  </p>
  <p>
    <%= f.label :to_manifestation_id -%><br />
    <%= f.text_field :to_manifestation_id -%>
  </p>
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>
</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @manifestation_has_manifestation -%></li>
    <li><%= link_to t('page.back'), manifestation_has_manifestations_path -%></li>
  </ul>
</div>
