<div id="content_detail">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.manifestation_has_manifestation')) -%></h1>
<div id="content_list">

<% form_for(@manifestation_has_manifestation) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.attributes.manifestation_has_manifestation.from_manifestation') -%><br />
    <%- if @manifestation_has_manifestation.from_manifestation -%>
      <%= link_to h(@manifestation_has_manifestation.from_manifestation.original_title), @manifestation_has_manifestation.from_manifestation -%>
      <%= f.hidden_field :from_manifestation_id -%>
    <%- else -%>
      <%= f.text_field :from_manifestation_id -%>
    <%- end -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.manifestation_has_manifestation.to_manifestation') -%><br />
    <%- if @manifestation_has_manifestation.to_manifestation -%>
      <%= link_to h(@manifestation_has_manifestation.to_manifestation.original_title), @manifestation_has_manifestation.to_manifestation -%>
      <%= f.hidden_field :to_manifestation_id -%>
    <%- else -%>
      <%= f.text_field :to_manifestation_id -%>
    <%- end -%>
  </p>

  <!--
  <p>
    <%= f.label :type -%><br />
    <%= f.text_field :type -%>
  </p>
  -->

  <p>
    <%= f.submit t('page.create') -%>
  </p>
<% end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.back'), manifestation_has_manifestations_path -%></li>
  </ul>
</div>
