<div id="manifestation">
<h2><%= t('page.listing', :model => t('activerecord.models.manifestation')) -%></h2>
<table class="index">
<%- @manifestations.each do |manifestation| -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td>
      <%= link_to manifestation.original_title, manifestation -%><br />
    </td>
  </tr>
<%- end -%>
</table>

<%= paginate(@manifestations, :param_name => :manifestation_page, :remote => true) -%>
</div>
