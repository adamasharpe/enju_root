<div id="submenu">
  <ul>
    <li><%= link_to t('page.back'), :back -%></li>
    <li><%= link_to t('page.listing', :model => t('activerecord.models.import_queue')), import_queues_path -%></li>
    <li><%= link_to t('work.add'), works_path -%></li>
    <li><%= link_to t('page.import_from_file'), '/page/import' -%></li>
  </ul>
</div>

<div id="content_detail">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.import_queue')) -%></h1>
<div id="content_list">

<%- form_for(@import_queue) do |f| -%>
  <%= f.error_messages -%>
  <p>
    <%= f.label t('activerecord.attributes.import_queue.isbn') -%><br />
    <%= f.text_field :isbn, :class => 'resource_isbn_issn' -%>

    <%= f.submit t('page.create') -%>
  </p>
  <%= javascript_tag("$('import_queue_isbn').focus()") -%>
<%- end -%>

</div>
</div>
