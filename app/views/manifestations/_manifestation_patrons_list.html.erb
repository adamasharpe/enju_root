    <h1>
      [M]
      <%= link_to h(@manifestation.original_title), @manifestation -%>
      <%= form_icon(@manifestation.manifestation_form) -%>
      <%- @manifestation.expressions.each do |e| -%>
        <%= link_to expression_form_icon(e.expression_form), expression_path(e) -%>
      <%- end -%>
    </h1>
    <p>
      <%- unless @manifestation.authors.blank? -%>
        <%= ('Author') -%>: <%= patrons_list(@manifestation.authors) -%>
      <%- end -%>
      <%- unless @manifestation.editors.blank? -%>
        <%= ('Editor') -%>: <%= patrons_list(@manifestation.editors) -%>
      <%- end -%>
      <%- unless @manifestation.publishers.blank? -%>
        <%= ('Publisher') -%>: <%= patrons_list(@manifestation.publishers) -%>
        <%- if @manifestation.date_of_publication -%>
          (<%=h @manifestation.date_of_publication.strftime('%Y') -%>)
        <%- end -%>
      <%- end -%>
      <%- unless @manifestation.access_address.blank? -%>
        <br />
        <%= link_to h(@manifestation.access_address), h(@manifestation.access_address) -%>
        <%= link_to image_tag('/icons/world_go.png', :size => '16x16', :alt => ('Web')), h(@manifestation.access_address) -%>
      <%- end -%>
    </p>
