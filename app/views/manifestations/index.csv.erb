title,creator,contributor,publisher,isbn,created_at,tag
<%- @manifestations.each do |manifestation| -%>
"<%= manifestation.original_title.gsub(/"/, '""') -%>",<%= manifestation.creator -%>,<%= manifestation.contributor -%>,<%= manifestation.publisher -%>,<%= manifestation.isbn -%>,<%= manifestation.created_at -%>,<%= manifestation.tags.each do |tag| tag.name end -%><%= "\n" -%>
<%- end -%>
