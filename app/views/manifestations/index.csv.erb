title,author,publisher,created_at,tag
<%- @manifestations.each do |manifestation| -%>
"<%=h manifestation.original_title.gsub(/"/, '""') -%>",<%=h manifestation.author -%>,<%=h manifestation.publisher -%>,<%=h manifestation.created_at -%>,<%= manifestation.tags.each do |tag| h(tag.name) end -%><%= "\n" -%>
<%- end -%>
