<%- unless @manifestation.has_single_work? -%>
  <%= render :partial => 'manifestations/work_list' -%>
<%- end -%>
<%- if @manifestation.original_manifestations.exists? or @manifestation.derived_manifestations.exists? -%>
  <%= render :partial => 'manifestations/manifestation_list' -%>
<%- end -%>
<%- unless @manifestation.questions(:user => current_user).empty? -%>
  <%= render :partial => 'manifestations/question_list', :locals => {:manifestation => @manifestation} -%>
<%- end -%>
<%- unless @xisbn_manifestations.blank? -%>
  <%= render :partial => 'manifestations/show_xisbn' -%>
<%- end -%>
<%- if current_user.try(:has_role?, 'Librarian') -%>
  <%= render :partial => 'history_list' -%>
<%- end -%>
