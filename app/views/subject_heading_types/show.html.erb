<div id="content_list">

<p>
  <b><%= t('activerecord.attributes.subject_heading_type.display_name') -%>:</b>
  <%=h @subject_heading_type.display_name.localize -%>
</p>

<p>
  <b><%= t('activerecord.attributes.subject_heading_type.note') -%>:</b>
  <%=h @subject_heading_type.note -%>
