<%- case when @manifestation -%>
  <%= render :partial => 'subjects/index_manifestation', :locals => {:subjects => @subjects} -%>
<%- when @classification -%>
  <%= render :partial => 'subjects/index_classification', :locals => {:subjects => @subjects} -%>
<%- else -%>
  <%= render :partial => 'subjects/index', :locals => {:subjects => @subjects} -%>
<%- end -%>
