<div id="submenu">
  <ul>
    <li><%= link_to t('page.new', :model => t('activerecord.models.expression_to_manifestation_rel_type')), new_expression_to_manifestation_rel_type_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1 class="title"><%= t('page.listing', :model => t('activerecord.models.expression_to_manifestation_rel_type')) -%></h1>
<div id="content_list">

<table class="index">
  <tr>
    <th id="position"></th>
    <th><%= t('activerecord.attributes.expression_to_manifestation_rel_type.name') -%></th>
    <th><%= t('activerecord.attributes.expression_to_manifestation_rel_type.display_name') -%></th>
    <th><%= t('activerecord.attributes.expression_to_manifestation_rel_type.note') -%></th>
  </tr>

<%- for expression_to_manifestation_rel_type in @expression_to_manifestation_rel_types -%>
  <tr class="line<%= cycle("0", "1") -%>">
    <td>
      <%- if expression_to_manifestation_rel_type.is_deletable_by(current_user) -%>
        <%= move_position(expression_to_manifestation_rel_type) -%>
      <%- end -%>
    </td>
    <td><%= link_to h(expression_to_manifestation_rel_type.name), expression_to_manifestation_rel_type -%></td>
    <td><%= link_to h(expression_to_manifestation_rel_type.display_name.localize), expression_to_manifestation_rel_type -%></td>
    <td><%=h expression_to_manifestation_rel_type.note -%></td>
    <td><%= link_to t('page.edit'), edit_expression_to_manifestation_rel_type_path(expression_to_manifestation_rel_type) -%></td>
    <td><%= link_to t('page.destroy'), expression_to_manifestation_rel_type, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<%- end -%>
</table>

</div>
