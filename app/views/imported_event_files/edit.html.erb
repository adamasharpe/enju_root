<div id="sidebar">
  <ul>
    <li><%= link_to t('page.show'), @imported_event_file -%></li>
    <li><%= link_to t('page.back'), imported_event_files_path -%></li>
  </ul>
</div>

<div class="inner">
  <h1 class="title"><%= t('page.editing', :model => t('activerecord.models.imported_event_file')) -%></h1>
<div class="inner">

<%- form_for(@imported_event_file) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :imported_event_file_name -%><br />
    <%= f.text_field :imported_event_file_name -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>
