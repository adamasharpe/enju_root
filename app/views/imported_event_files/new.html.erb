<div id="sidebar">
  <ul>
    <li><%= link_to t('page.back'), imported_event_files_path -%></li>
  </ul>
</div>

<div class="inner">
  <h1 class="title"><%= t('page.new', :model => t('activerecord.models.imported_event_file')) -%></h1>
<div class="inner">

<%- form_for(@imported_event_file, :html => { :multipart => true }) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <b><%= t('page.file') -%></b><br />
    <%= f.file_field :imported_event -%>
  </p>

  <p>
    <%= f.submit t('page.create') -%>
  </p>
<%- end -%>

</div>
</div>
