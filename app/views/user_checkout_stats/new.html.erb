<div id="submenu">
  <ul>
    <li><%= link_to t('page.back'), user_checkout_stats_path -%></li>
  </ul>
</div>

<div id="content_detail">
<div id="content_list">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.user_checkout_stat')) -%></h1>

<%- form_for(@user_checkout_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.attributes.user_checkout_stat.start_date') -%><br />
    <%= f.datetime_select :start_date -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.user_checkout_stat.end_date') -%><br />
    <%= f.datetime_select :end_date -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.user_checkout_stat.note') -%><br />
    <%= f.text_area :note -%>
  </p>
  <p>
    <%= f.submit t('page.create') -%>
  </p>
<%- end -%>

</div>
</div>
