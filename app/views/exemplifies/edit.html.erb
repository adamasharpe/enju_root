<div id="sidebar">
  <ul>
    <li><%= link_to t('page.show'), @exemplify -%></li>
    <li><%= link_to t('page.back'), exemplifies_path -%></li>
  </ul>
</div>

<div id="main">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.exemplify')) -%></h1>
<div class="inner">

<%- form_for(@exemplify) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.models.manifestation') -%><br />
    <%= f.text_field :manifestation_id -%>
  </p>
  <p>
    <%= f.label t('activerecord.models.item') -%><br />
    <%= f.text_field :item_id -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>
