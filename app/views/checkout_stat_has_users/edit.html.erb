<h1><%= t('page.editing', :model => t('activerecord.models.checkout_stat_has_user')) -%></h1>

<% form_for(@checkout_stat_has_user) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :user_checkout_stat_id -%><br />
    <%= f.text_field :user_checkout_stat_id -%>
  </p>
  <p>
    <%= f.label :user_id -%><br />
    <%= f.text_field :user_id -%>
  </p>
  <p>
    <%= f.label :checkouts_count -%><br />
    <%= f.text_field :checkouts_count -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>

<%= link_to t('page.show'), @checkout_stat_has_user -%> |
<%= link_to t('page.back'), checkout_stat_has_users_path -%>
