<h1><%= t('page.listing', :model => t('activerecord.models.user_reserve_stat')) -%></h1>

<table>
  <tr>
    <th><%= t('activerecord.attributes.user_reserve_stat.start_date') -%></th>
    <th><%= t('activerecord.attributes.user_reserve_stat.end_date') -%></th>
    <th><%= t('activerecord.attributes.user_reserve_stat.note') -%></th>
    <th>State</th>
  </tr>

<% for user_reserve_stat in @user_reserve_stats -%>
  <tr>
    <td><%=h user_reserve_stat.start_date -%></td>
    <td><%=h user_reserve_stat.end_date -%></td>
    <td><%=h user_reserve_stat.note -%></td>
    <td><%=h user_reserve_stat.state -%></td>
    <td><%= link_to t('page.show'), user_reserve_stat -%></td>
    <td><%= link_to t('page.edit'), edit_user_reserve_stat_path(user_reserve_stat) -%></td>
    <td><%= link_to t('page.destroy'), user_reserve_stat, :confirm => t('page.are_you_sure'), :method => :delete -%></td>
  </tr>
<% end -%>
</table>

<br />

<%= link_to t('page.new', :model => t('activerecord.models.user_reserve_stat')), new_user_reserve_stat_path -%>
