<h1>Editing user_reserve_stat</h1>

<% form_for(@user_reserve_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :from_date -%><br />
    <%= f.datetime_select :from_date -%>
  </p>
  <p>
    <%= f.label :to_date -%><br />
    <%= f.datetime_select :to_date -%>
  </p>
  <p>
    <%= f.label :note -%><br />
    <%= f.text_area :note -%>
  </p>
  <p>
    <%= f.label :state -%><br />
    <%= f.text_field :state -%>
  </p>
  <p>
    <%= f.submit "Update" -%>
  </p>
<% end -%>

<%= link_to 'Show', @user_reserve_stat -%> |
<%= link_to 'Back', user_reserve_stats_path -%>
