<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.user_reserve_stat')) -%></h1>

<% form_for(@user_reserve_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :start_date -%><br />
    <%= f.datetime_select :start_date -%>
  </p>
  <p>
    <%= f.label :end_date -%><br />
    <%= f.datetime_select :end_date -%>
  </p>
  <p>
    <%= f.label :note -%><br />
    <%= f.text_area :note -%>
  </p>
  <p>
    <%= f.label :state -%><br />
    <%= f.text_field :state -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<% end -%>

<%= link_to t('page.show'), @user_reserve_stat -%> |
<%= link_to t('page.back'), user_reserve_stats_path -%>
