<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.user_has_shelf')) -%></h1>
<div id="content_list">

<%= form_for(@user_has_shelf) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.models.user') -%><br />
    <%= f.text_field :user_id -%>
  </p>
  <p>
    <%= f.label t('activerecord.models.shelf') -%><br />
    <%= f.text_field :shelf_id -%>
  </p>
  <p>
    <%= f.submit t('page.create') -%>
  </p>
<%- end -%>
</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), user_has_shelves_path -%></li>
  </ul>
</div>
