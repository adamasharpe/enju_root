<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.barcode')) -%></h1>
<div id="content_list">

<%- form_for(@barcode) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label :barcode_type -%><br />
    <%= f.text_field :barcode_type -%>
  </p>
  <p>
    <%= f.label :barcodable_id -%><br />
    <%= f.text_field :barcodable_id -%>
  </p>
  <p>
    <%= f.label :barcodable_type -%><br />
    <%= f.text_field :barcodable_type -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @barcode -%></li>
    <li><%= link_to t('page.back'), barcodes_path -%></li>
  </ul>
</div>
