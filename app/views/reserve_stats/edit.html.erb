<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @reserve_stat %></li>
    <li><%= link_to 'Back', reserve_stats_path %></li>
  </ul>
</div>

<div id="content_detail">
<h1>Editing reserve_stat</h1>

<% form_for(@reserve_stat) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :from_date %><br />
    <%= f.datetime_select :from_date %>
  </p>
  <p>
    <%= f.label :to_date %><br />
    <%= f.datetime_select :to_date %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

</div>
