<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.bookmark_stat')) -%></h1>
<div id="content_list">

<%= form_for(@bookmark_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.attributes.bookmark_stat.start_date') -%><br />
    <%= f.datetime_select :start_date -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.bookmark_stat.end_date') -%><br />
    <%= f.datetime_select :end_date -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.bookmark_stat.note') -%><br />
    <%= f.text_area :note -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.show'), @bookmark_stat -%></li>
    <li><%= link_to t('page.back'), bookmark_stats_path -%></li>
  </ul>
</div>
