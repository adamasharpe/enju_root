<div id="submenu">
  <ul>
    <li><%= link_to t('show'), @patron_merge -%></li>
    <li><%= link_to ('Back'), patron_merges_path -%></li>
  </ul>
</div>

<div id="content_detail">
<h1><%= ('Editing patron_merge') -%></h1>

<%= error_messages_for :patron_merge -%>

<%- form_for(@patron_merge) do |f| -%>

  <p>
  <b><%= ('Patron') -%></b><br />
  <%= f.text_field :patron_id, :value => @patron_merge.patron_id -%>
  </p>

  <p>
  <b><%= ('Patron merge list') -%></b><br />
  <%= f.text_field :patron_merge_list_id, :value => @patron_merge.patron_merge_list_id -%>
  </p>

  <p>
    <%= f.submit t('update') -%>
  </p>
<%- end -%>

</div>
