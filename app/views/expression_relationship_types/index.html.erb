<h1>Listing expression_relationship_types</h1>

<table id="index">
  <tr>
    <th>Name</th>
    <th>Display name</th>
    <th>Note</th>
    <th>Position</th>
  </tr>

<% for expression_relationship_type in @expression_relationship_types %>
  <tr>
    <td><%=h expression_relationship_type.name %></td>
    <td><%=h expression_relationship_type.display_name %></td>
    <td><%=h expression_relationship_type.note %></td>
    <td><%=h expression_relationship_type.position %></td>
    <td><%= link_to 'Show', expression_relationship_type %></td>
    <td><%= link_to 'Edit', edit_expression_relationship_type_path(expression_relationship_type) %></td>
    <td><%= link_to 'Destroy', expression_relationship_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New expression_relationship_type', new_expression_relationship_type_path %>
