<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.new', :model => t('activerecord.models.expression')) -%></h1>
<div id="content_list">

<%= form_for(:expression, :url => work_expressions_path(@work)) do |f| -%>
  <%= f.error_messages -%>

  <%= render :partial => 'form', :locals => {:f => f} -%>
  <div class="actions">
    <%= f.submit -%>
  </div>
<%- end -%>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), work_expressions_path(@work) -%></li>
  </ul>
</div>
